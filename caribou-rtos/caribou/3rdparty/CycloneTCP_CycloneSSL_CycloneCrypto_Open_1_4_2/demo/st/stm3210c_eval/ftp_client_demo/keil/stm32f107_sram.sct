;*******************************************************************
;* STM32F107 linker scatter file (running code from internal SRAM) *
;*******************************************************************

LR_IROM1 0x20000000 0x10000
{
   ER_IROM1 0x20000000
   {
      *.o (RESET, +First)
      *(InRoot$$Sections)
      .ANY (+RO)
   }
   
   RW_IRAM1 +0
   { 
      .ANY (+RW +ZI)
   }
   
   ScatterAssert((ImageLength(ER_IROM1) + ImageLength(RW_IRAM1)) < 0xB800)
   
   ARM_LIB_STACK 0x2000C000 EMPTY -0x800
   {
   }
   
   ARM_LIB_HEAP 0x2000C000 EMPTY 0x4000
   {
   }
}
