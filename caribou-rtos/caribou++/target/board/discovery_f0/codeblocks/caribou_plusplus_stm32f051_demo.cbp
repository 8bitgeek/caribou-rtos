<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="CARIBOU++ STM32F051 Demo" />
		<Option pch_mode="2" />
		<Option compiler="armelfgcc" />
		<Build>
			<Target title="Debug">
				<Option output="../obj/Debug/caribou_demo.elf" prefix_auto="0" extension_auto="0" />
				<Option working_dir="../" />
				<Option object_output="obj/Debug/" />
				<Option external_deps="obj/Debug/libcaribou.a;" />
				<Option type="1" />
				<Option compiler="armelfgcc" />
				<ExtraCommands>
					<Add after="$(OBJCOPY)  --output-target=ihex ../obj/Debug/$(TARGET).elf ../obj/Debug/$(TARGET).hex" />
					<Add after="$(OBJCOPY)  --output-target=srec ../obj/Debug/$(TARGET).elf ../obj/Debug/$(TARGET).s19" />
				</ExtraCommands>
			</Target>
			<Target title="Release">
				<Option output="../obj/Release/caribou_demo.elf" prefix_auto="0" extension_auto="0" />
				<Option working_dir="../" />
				<Option object_output="obj/Release/" />
				<Option external_deps="obj/Release/libcaribou.a;" />
				<Option type="1" />
				<Option compiler="armelfgcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
			<Target title="Upload">
				<Option output="caribou_demo.elf" prefix_auto="1" extension_auto="0" />
				<Option working_dir="../" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="armelfgcc" />
				<ExtraCommands>
					<Add after="$(PREFIX)gdb -x driver_stm32f051/sh/upload.gdb" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="OBJCOPY" value='&quot;$(PREFIX)objcopy&quot;' />
				<Variable name="TARGET" value="caribou_demo" />
				<Variable name="PREFIX" value="arm-none-eabi-" />
				<Variable name="VECTOR_TABLE" value="_vectors" />
				<Variable name="DEBUGGER_FLAGS" value='&quot;-x init.gdb&quot;' />
			</Environment>
		</Build>
		<Compiler>
			<Add option="-mthumb" />
			<Add option="-mcpu=cortex-m0" />
			<Add option="-msoft-float" />
			<Add option="-mlittle-endian" />
			<Add option="-mapcs-frame" />
			<Add option="-w" />
			<Add option="-g" />
			<Add option="-pipe" />
			<Add option="-ggdb" />
			<Add option="-std=gnu99" />
			<Add option="-fno-builtin" />
			<Add option="-fno-unwind-tables" />
			<Add option="-fno-use-cxa-atexit" />
			<Add option="-fno-math-errno" />
			<Add option="-fmerge-all-constants" />
			<Add option="-fno-rtti" />
			<Add option="-fno-exceptions" />
			<Add option="-DDRIVER_STM32F051" />
			<Add directory="../include" />
			<Add directory="../../../cpu/arm/cortex-m0" />
			<Add directory="../../../chip/stm32/stm32f051/include" />
			<Add directory="../../../../include" />
		</Compiler>
		<Linker>
			<Add option="-nostartfiles" />
			<Add option="-mthumb" />
			<Add option="-mno-thumb-interwork" />
			<Add option="-mcpu=cortex-m0" />
			<Add option="-msoft-float" />
			<Add option="-mapcs-frame" />
			<Add option="-mlittle-endian" />
			<Add option="-Xlinker --undefined=$(VECTOR_TABLE)" />
			<Add option="-Xlinker -Map=$(TARGET).map" />
			<Add option="-T../ld/caribou-rom.ld" />
			<Add option="-lgcc" />
			<Add library="../obj/Debug/libcaribou.a" />
			<Add directory="../include" />
			<Add directory="../../../cpu/arm/cortex-m0" />
			<Add directory="../../../chip/stm32/stm32f051/include" />
			<Add directory="../../../../include" />
		</Linker>
		<Unit filename="../../../../demo/include/caribou++demo.h" />
		<Unit filename="../../../../demo/src/caribou++demo.cpp" />
		<Unit filename="../ld/caribou-rom.ld" />
		<Unit filename="../sh/init.gdb" />
		<Unit filename="../sh/start_openocd.sh" />
		<Unit filename="../sh/stm32f051.cfg" />
		<Unit filename="../sh/upload.gdb" />
		<Unit filename="../sh/upload.sh" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger>
				<search_path add="./u" />
				<search_path add="./" />
				<search_path add="../" />
				<search_path add="../src" />
				<search_path add="../driver_stm32f051/src" />
				<remote_debugging>
					<options conn_type="0" serial_baud="115200" additional_cmds_before="source driver_stm32f051/sh/init.gdb&#x0A;" skip_ld_path="1" />
				</remote_debugging>
			</debugger>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
