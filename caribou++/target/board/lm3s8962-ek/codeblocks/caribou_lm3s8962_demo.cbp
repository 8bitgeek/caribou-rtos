<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="caribou lm3s8962 demo" />
		<Option pch_mode="2" />
		<Option compiler="armelfgcc" />
		<Build>
			<Target title="Debug">
				<Option output="../obj/Debug/caribou_demo.elf" prefix_auto="1" extension_auto="0" />
				<Option working_dir="../" />
				<Option object_output="obj/Debug/" />
				<Option external_deps="obj/Debug/libcaribou++.a" />
				<Option type="1" />
				<Option compiler="armelfgcc" />
				<Compiler>
					<Add option="-ggdb" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="../obj/Debug/caribou_demo.elf" prefix_auto="1" extension_auto="0" />
				<Option working_dir="../" />
				<Option object_output="obj/Debug/" />
				<Option external_deps="obj/Debug/libcaribou++.a" />
				<Option type="1" />
				<Option compiler="armelfgcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
			</Target>
			<Environment>
				<Variable name="OBJCOPY" value='&quot;$(PREFIX)objcopy&quot;' />
				<Variable name="FORMAT" value="ihex" />
				<Variable name="TARGET" value="caribou_demo" />
				<Variable name="PREFIX" value="arm-none-eabi-" />
				<Variable name="VECTOR_TABLE" value="_vectors" />
				<Variable name="DEBUGGER_FLAGS" value='&quot;-x init.gdb&quot;' />
			</Environment>
		</Build>
		<Compiler>
			<Add option="-mcpu=cortex-m3" />
			<Add option="-fdata-sections" />
			<Add option="-ffunction-sections" />
			<Add option="-mthumb" />
			<Add option="-mno-thumb-interwork" />
			<Add option="-msoft-float" />
			<Add option="-pipe" />
			<Add option="-fno-builtin" />
			<Add option="-fno-math-errno" />
			<Add option="-fmerge-all-constants" />
			<Add option="-march=armv7-m" />
			<Add option="-mfix-cortex-m3-ldrd" />
			<Add option="-falign-functions=2" />
			<Add option="-fno-rtti" />
			<Add option="-fno-exceptions" />
			<Add option="-DNEW_TAKES_SIZE_T" />
			<Add option="-DDRIVER_LM3S8962" />
			<Add option="-DCPU_CORTEX_M3" />
			<Add option="-DCARIBOU_NETWORKING=1" />
			<Add directory="../include" />
			<Add directory="../../../../demo/include" />
			<Add directory="../../../cpu/arm/cortex-m3" />
			<Add directory="../../../chip/stellaris/lm3s8962/include" />
			<Add directory="../../../../include" />
			<Add directory="../../../../../caribou/include" />
			<Add directory="../../../../../caribou/target/cpu/arm/cortex-m3" />
			<Add directory="../../../../../caribou/target/chip/stellaris/lm3s8962/include" />
			<Add directory="../../../../../caribou/target/board/lm3s8962-ek/include" />
		</Compiler>
		<Linker>
			<Add option="-nostartfiles" />
			<Add option="-Xlinker --undefined=$(VECTOR_TABLE)" />
			<Add option="-Xlinker -Map=$(TARGET).map" />
			<Add option="-T../ld/caribou-rom.ld" />
			<Add library="../obj/Debug/libcaribou++.a" />
			<Add library="../../../../../caribou/target/board/lm3s8962-ek/obj/Debug/libcaribou.a" />
			<Add directory="../include" />
			<Add directory="../../../cpu/arm/cortex-m3" />
			<Add directory="../../../chip/stellaris/lm3s8962/include" />
			<Add directory="../../../../include" />
		</Linker>
		<ExtraCommands>
			<Add after="$(OBJCOPY) --output-target=ihex ../obj/Debug/$(TARGET).elf ../obj/Debug/$(TARGET).hex" />
			<Add after="$(OBJCOPY) --output-target=srec ../obj/Debug/$(TARGET).elf ../obj/Debug/$(TARGET).s19" />
		</ExtraCommands>
		<Unit filename="../../../../demo/include/caribou++demo.h" />
		<Unit filename="../../../../demo/src/caribou++demo.cpp" />
		<Unit filename="../../../../demo/src/main.cpp" />
		<Unit filename="../ld/caribou-rom.ld" />
		<Unit filename="../sh/init.gdb" />
		<Unit filename="../sh/upload.gdb" />
		<Unit filename="../sh/upload.sh" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger>
				<search_path add="../include" />
				<search_path add="../../../cpu/arm/cortex-m3" />
				<search_path add="../../../chip/stellaris/lm3s8962/include" />
				<search_path add="../../../../include" />
				<remote_debugging>
					<options conn_type="0" serial_baud="115200" additional_cmds_before="source sh/init.gdb&#x0A;" skip_ld_path="1" />
				</remote_debugging>
			</debugger>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
