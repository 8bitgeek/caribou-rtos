<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="CARIBOU LPC2468 Demo" />
		<Option pch_mode="2" />
		<Option compiler="armelfgcc" />
		<Build>
			<Target title="Debug">
				<Option output="../obj/Debug/caribou_demo.elf" prefix_auto="0" extension_auto="0" />
				<Option working_dir="../" />
				<Option object_output="obj/Debug/" />
				<Option external_deps="obj/Debug/libcaribou.a;" />
				<Option type="1" />
				<Option compiler="armelfgcc" />
				<ExtraCommands>
					<Add after="$(OBJCOPY) --image-base 0x80000000 --output-target=ihex ../obj/Debug/$(TARGET).elf ../obj/Debug/$(TARGET).hex" />
					<Add after="$(OBJCOPY) --image-base 0x80000000 --output-target=srec ../obj/Debug/$(TARGET).elf ../obj/Debug/$(TARGET).s19" />
				</ExtraCommands>
			</Target>
			<Target title="Release">
				<Option output="../obj/Release/caribou_demo.elf" prefix_auto="0" extension_auto="0" />
				<Option working_dir="../" />
				<Option object_output="obj/Release/" />
				<Option external_deps="obj/Release/libcaribou.a;" />
				<Option type="1" />
				<Option compiler="armelfgcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
			<Environment>
				<Variable name="OBJCOPY" value='&quot;$(PREFIX)objcopy&quot;' />
				<Variable name="TARGET" value="caribou_demo" />
				<Variable name="PREFIX" value="arm-none-eabi-" />
				<Variable name="DEBUGGER_FLAGS" value='&quot;-x init.gdb&quot;' />
			</Environment>
		</Build>
		<Compiler>
			<Add option="-msoft-float" />
			<Add option="-mlittle-endian" />
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="-ggdb" />
			<Add option="-fno-common" />
			<Add option="-fno-exceptions" />
			<Add option="-fno-use-cxa-atexit" />
			<Add option="-fno-math-errno" />
			<Add option="-fmerge-all-constants" />
			<Add option="-fno-rtti" />
			<Add option="-mcpu=arm7tdmi-s" />
			<Add option="-std=gnu99" />
			<Add option="-fno-builtin" />
			<Add option="-fno-unwind-tables" />
			<Add option="-ffunction-sections" />
			<Add option="-fdata-sections" />
			<Add directory="../include" />
			<Add directory="../../../cpu/arm/arm7tdmi" />
			<Add directory="../../../chip/nxp/lpc2468/include" />
			<Add directory="../../../../include" />
		</Compiler>
		<Linker>
			<Add option="-Wl,-Map,Debug/map.txt" />
			<Add option="-mcpu=arm7tdmi-s" />
			<Add option="-mapcs-frame" />
			<Add option="-ggdb" />
			<Add option="-std=gnu99" />
			<Add option="-fno-builtin" />
			<Add option="-nostartfiles" />
			<Add option="-msoft-float" />
			<Add option="-Xlinker -Map=$(TARGET).map" />
			<Add option="-ffunction-sections" />
			<Add option="-fdata-sections" />
			<Add option="-Wl,--gc-sections" />
			<Add option="-T../ld/caribou-rom.ld" />
			<Add option="-Xlinker --undefined=_vectors" />
			<Add library="../obj/Debug/libcaribou.a" />
			<Add directory="../include" />
			<Add directory="../../../cpu/arm/arm7tdmi" />
			<Add directory="../../../chip/nxp/lpc2468/include" />
			<Add directory="../../../../include" />
		</Linker>
		<Unit filename="../../../../demo/caribou_demo.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="../ld/caribou-rom.ld" />
		<Unit filename="../sh/init.gdb" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger>
				<search_path add="./" />
				<search_path add="../" />
				<search_path add="../src" />
				<remote_debugging>
					<options conn_type="0" serial_baud="115200" additional_cmds_before="source sh/init.gdb&#x0A;" skip_ld_path="1" />
				</remote_debugging>
			</debugger>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
